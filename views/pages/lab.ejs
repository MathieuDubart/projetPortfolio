<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/heads/homeHead'); %>
    <%- include('../partials/heads/labHead'); %>
</head>
<body class="container-lg">

    <header>
        <%- include('../partials/layout/header'); %>
    </header>
    <div id="socialMedias">
        <%- include('../partials/socialMedia'); %>
    </div>

    <div class="row d-flex justify-content-center align-items-center">
        <div class="col-xxl-12 d-flex justify-content-start lab-title"> 
            <h1><%- parsed_setting_desc.menu[1] %></h1>
        </div>
    </div>
    
    <div class="row d-flex lab">

        <% let colBool = true;
            let labLength = parsed_lab_desc.images.length;
            for(j=0; j < labLength; j++) {
                if (colBool==true) { %>
                    <div class="col-md-0 col-lg-3 d-flex justify-content-center align-items-center "></div>
                    
                    <div class="col-md-6 col-lg-3 d-flex justify-content-center align-items-center lab-img">
                        <img src="<%- parsed_lab_desc.images[j] %>">
                    </div>

                <% colBool = false;
                    } else {%>
                    <div class="col-md-6 col-lg-3 d-flex justify-content-center align-items-center lab-img">
                        <img src="<%- parsed_lab_desc.images[j] %>">
                    </div>
                    <div class="col-md-0 col-lg-3 d-flex justify-content-center align-items-center"></div>
                <% colBool = true;
                }
            } %>
    </div>


    
    <footer class="container-fluid d-flex justify-content-center align-tems-center footer">
        <%- include('../partials/layout/footer'); %>
    </footer>

    <script src="/script.js"></script>
</body>
</html>
