<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
<link rel="stylesheet" href="/style/style.css">
<link rel="stylesheet" href="/style/socialMedia.css">
<%- parsed_setting_desc.font_link[0] %>
<style>
    :root {

        --custom-loader-background-color: <%- parsed_setting_desc.loader_bgcolor[0] %>;
        --custom-title-font: <%- parsed_setting_desc.font_name[0] %>;
        --custom-p-font: <%- parsed_setting_desc.font_name[1] %>;

        --custom-background-image: url('<%- parsed_setting_desc.background_image[0] %>');
        --custom-background-repeat: <%- parsed_setting_desc.background_repeat %>;
        --custom-background-position: <%- parsed_setting_desc.background_position %>;
        --custom-background-attachment: <%- parsed_setting_desc.background_attachment %>;
        --custom-background-size: <%- parsed_setting_desc.background_size %>;

        --custom-background-color: <%- parsed_setting_desc.background_color %>;

        --custom-accent-color: <%- parsed_setting_desc.accent_color %>;

        --custom-h1-color: <%- parsed_setting_desc.h1[0] %>;
        --custom-h2-color: <%- parsed_setting_desc.h2[0] %>;

        --custom-h1-size: <%- parsed_setting_desc.h1[1] %>;
        --custom-h1-responsive-size: calc(var(--custom-h1-size) * 1.5);
        --custom-h2-size: <%- parsed_setting_desc.h2[1] %>;
        --custom-h2-responsive-size: calc(var(--custom-h2-size) * 1.5);

        
        --custom-h1-weight: <%- parsed_setting_desc.h1[2] %>;
        --custom-h2-weight: <%- parsed_setting_desc.h2[2] %>;

        --custom-h1-letter-spacing: <%- parsed_setting_desc.h1[3] %>;
        --custom-h2-letter-spacing: <%- parsed_setting_desc.h2[3] %>;

        --custom-p-color: <%- parsed_setting_desc.paragraph[0] %>;
        --custom-p-size: <%- parsed_setting_desc.paragraph[1] %>;
        --custom-p-responsive-size: calc(var(--custom-p-size) * 2);
        --custom-p-weight: <%- parsed_setting_desc.paragraph[2] %>;

        --custom-tag-color: <%- parsed_setting_desc.tags[0] %>;
        --custom-tag-size: <%- parsed_setting_desc.tags[1] %>;
        --custom-tag-responsive-size: calc(var(--custom-tag-size) * 1.5);
        --custom-tag-weight: <%- parsed_setting_desc.tags[2] %>;

        --custom-filter-button-inactive-color: <%- parsed_setting_desc.filter_button[0] %>;
        --custom-filter-button-active-color: <%- parsed_setting_desc.filter_button[1] %>;
        --custom-filter-button-size: <%- parsed_setting_desc.filter_button[2] %>;
        --custom-filter-button-responsive-size: calc(var(--custom-filter-button-size) * 1.5);
        --custom-filter-button-weight: <%- parsed_setting_desc.filter_button[3] %>;

        --custom-button-typo-color: <%- parsed_setting_desc.button_typo[0] %>;
        --custom-button-typo-size: <%- parsed_setting_desc.button_typo[1] %>;
        --custom-button-typo-responsive-size: calc(var(--custom-button-typo-size) *1.5);
        --custom-button-typo-weight: <%- parsed_setting_desc.button_typo[2] %>;

        --custom-button-container-bg-color: <%- parsed_setting_desc.button_container[0] %>;
        --custom-button-container-border: <%- parsed_setting_desc.button_container[1] %>;
        --custom-button-container-border-radius: <%- parsed_setting_desc.button_container[2] %>;

        --custom-span-color: <%- parsed_setting_desc.span[0] %>;
        --custom-span-weight: <%- parsed_setting_desc.span[1] %>;

        --custom-footer-background-color: <%- parsed_setting_desc.footer_bgcolor %>;
        --custom-footer-color: <%- parsed_setting_desc.footer_color %>;
    }
</style>