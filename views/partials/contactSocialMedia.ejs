<div class="row d-flex mailInfo">
    <div class="row col-12 socialMediaIcons">
        <div class="col-1">
            <a href="mailto:<%- parsed_bio.email %>" target="_blank">
                <img src="https://icon-library.com/images/e-mail-icon-png/e-mail-icon-png-11.jpg">
            </a>
        </div>
        <div class="col-11 d-flex justify-content-start align-items-center">
            <p> <%- parsed_bio.email %></p>
        </div>
    </div>
</div>

<div class="row d-flex linkedInInfo">
    <div class="row col-12 socialMediaIcons">
        <div class="col-1">
            <a href="<%- userInfos.links.linkedin %>" target="_blank">
                <img src="https://cdn-icons-png.flaticon.com/512/61/61109.png">
            </a>
        </div>
        <div class="col-11 d-flex justify-content-start align-items-center">
            <p> <%- parsed_bio.linkedin %> </p>
        </div>
    </div>
</div>

<div class="row d-flex instagramInfo">
    <div class="row col-12 socialMediaIcons">
        <div class="col-1">
            <a href="<%- userInfos.links.instagram %>" target="_blank">
                <img src="https://cdn-icons-png.flaticon.com/512/87/87390.png">
            </a>
        </div>
        <div class="col-11 d-flex justify-content-start align-items-center">
            <p><%- parsed_bio.instagram %></p>
        </div>
    </div>
</div>
